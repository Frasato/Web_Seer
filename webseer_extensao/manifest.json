{
  "manifest_version": 3,
  "name": "Minha Primeira Extensão",
  "version": "1.0",
  "description": "Uma extensão simples de exemplo.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": [
    "http://localhost:8080/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}